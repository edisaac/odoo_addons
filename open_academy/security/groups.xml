<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>

        <record model="ir.module.category" id="open_academic_module">
            <field name="name">OpenAcademic_Module</field>
        </record>

        <record model="res.groups" id="user_grupo">
            <field name="name">Users</field>
            <field name="category_id" ref="open_academic_module" />

        </record>

         <record model="res.groups" id="manager_grupo">
            <field name="name">Manager</field>
            <field name="category_id" ref="open_academic_module" />
            <field name="implied_ids" eval="{(4,ref('user_grupo'))}"/>
             <!--opcion 4 vincula-->
        </record>
    </data>
</openerp>